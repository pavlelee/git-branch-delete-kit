{
	"name": "git-branch-delete-kit",
	"displayName": "Git branch delete kit",
	"description": "Ask whether to delete the remote branch while deleting the local branch.",
	"version": "0.0.2",
	"publisher": "pavlelee",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:git-branch-delete-kit.deleteBranch"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "git-branch-delete-kit.deleteBranch",
				"title": "GBDK: Delete Branch"
			}
		]
	},
	"extensionDependencies": [
		"vscode.git"
	],
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"@types/vscode": "^1.46.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {}
}
